{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 2,
    "target": "es5",
    "lib": [
      "ES2015",
      "DOM"
    ],
  },
  "include": [
    "src/js/**/*",
    "src/icons/*"
  ]
}