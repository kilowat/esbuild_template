{
  "name": "esbuild",
  "version": "1.0.0",
  "description": "CSS and JavaScript build system using esbuild",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "bundle": "node ./esbuild.config.js",
    "watch:prod": "npm run bundle -- prod watch",
